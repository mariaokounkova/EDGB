          RowBox[{"i", ",", 
           RowBox[{
            RowBox[{"CIxs", "[", "n", "]"}], "[", 
            RowBox[{"[", "j", "]"}], "]"}]}], "]"}], 
         RowBox[{"\[Phi]\[Phi]", "[", 
          RowBox[{"[", 
           RowBox[{"i", "+", "1"}], "]"}], "]"}]], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "0", ",", 
          RowBox[{"n", "-", "1"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "n"}], "}"}]}], "]"}]}]}], 
