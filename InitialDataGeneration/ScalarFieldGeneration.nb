Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"LGLC", "[", "n_", "]"}], "[", 
   RowBox[{"j_", ",", "x_"}], "]"}], ":=", 
  RowBox[{
   FractionBox[
    RowBox[{"-", 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       SuperscriptBox["x", "2"]}], ")"}]}], 
    RowBox[{"n", 
     RowBox[{"(", 
      RowBox[{"n", "+", "1"}], ")"}], 
     RowBox[{"LegendreP", "[", 
      RowBox[{"n", ",", 
       RowBox[{
        RowBox[{"LGLx", "[", "n", "]"}], "[", "j", "]"}]}], "]"}], 
     RowBox[{"(", 
      RowBox[{"x", "-", 
       RowBox[{
        RowBox[{"LGLx", "[", "n", "]"}], "[", "j", "]"}]}], ")"}]}]], 
   RowBox[{
    RowBox[{
     RowBox[{"Derivative", "[", 
      RowBox[{"0", ",", "1"}], "]"}], "[", "LegendreP", "]"}], "[", 
    RowBox[{"n", ",", "x"}], 
    "]"}]}]}]], \
"Input",ExpressionUUID->"8fab5ce8-726e-4976-9cc0-9b951f8eb1ed"],

